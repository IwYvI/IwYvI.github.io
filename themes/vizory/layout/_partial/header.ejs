<div class="background">
  <div class="ribbon"></div>
  <div class="tableCard"></div>
</div>
<div class="nav">
  <table>
    <tr>
      <% for (var i in theme.menu){ %>
        <td><a href="<%- url_for(theme.menu[i].directory) %>"><%-  __(theme.menu[i].page)%></a></td>
      <% } %>
    </tr>
    <% for (var j = 0; j < 4; j++){ %>
      <tr>
        <% for (var i in theme.menu){ %>
          <td></td>
        <% } %>
      </tr>
    <% } %>
  </table>
</div>
<div class="container">
  <div class="header">
    <div class="headerBackground">
      <div class="headerBackgroundImg"></div>
    </div>
    <div class="headerContent">
      <div class="avatar" alt="avatar">
        <a href="<%- url_for() %>">
          <img src="/img/avatar.jpg" alt="avatar">
        </a>
      </div>
      <div class="blogTitle">
        <h1><%- config.title %></h1>
        <p><%- config.subtitle %></p>
      </div>
    </div>
  </div>
  <div class="main">
    <div class="mainBackground">
      <div class="mainBackgroundLeft"></div>
      <div class="mainBackgroundRight">
        <div class="mainBackgroundCode">
          <pre>html {
  width: 100%;
  height: 100%; }

body {
  width: 100%;
  height: 100%;
  background-color: #333333;
  position: relative;
  padding-top: 1px;
  margin-top: -1px; }

.background {
  width: 100%;
  height: 100%; }</pre>
        </div>
      </div>
    </div>
